          <!--Ventana Modal-->
<div class="modal  fade" tabindex="-1" role="dialog" id="ventana">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title">Registro Usuario</h4>
      </div>
      <div class="modal-body">            
            <div class="form-horizontal">          
            <!--Id oculto para realizar la modificacion-->
            <input type="text" id="txtId" hidden="true"   />
                    	
                     <div class="form-group">
                            <label class="col-md-2">
                                Cedula</label>
                            <div class="col-md-8">
                                <input type="text" id="txtCedula" class="form-control    validator" group-validator="grupo1" requiredFm="true"  validatorMssg="Por favor digitar la cedula"/>
                            </div>
                      </div>

                      <div class="form-group">
                            <label class="col-md-2">
                                Nombre</label>
                            <div class="col-md-8">
                                <input type="text" id="txtNombre" class="form-control    validator" group-validator="grupo1" requiredFm="true"  validatorMssg="Por favor digitar el nombre"/>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-2">
                                Apellido</label>
                            <div class="col-md-8">
                                <input type="text" id="txtApellido" class="form-control    validator" group-validator="grupo1" requiredFm="true"  validatorMssg="Por favor digitar el apellido"/>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-2">
                               Email</label>
                            <div class="col-md-8">
                                <input type="text" id="txtEmail" class="form-control     validator " group-validator="grupo1" requiredFm="true" validatorMssg="Por favor digitar su enail"/>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-2">
                                Login</label>
                            <div class="col-md-8">
                                <input type="text" id="txtLogin" class="form-control    validator" group-validator="grupo1" requiredFm="true"  validatorMssg="Por favor digitar login"/>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-2">
                                Email</label>
                            <div class="col-md-8">
                                <input type="text" id="txtEmail" class="form-control    validator" group-validator="grupo1" requiredFm="true"  validatorMssg="Por favor digitar el email" email="true"/>
                            </div>
                        </div>

                        
						<div class="form-group">
                            <label class="col-md-2">
                                Password</label>
                            <div class="col-md-8">
                                <input  id="txtPass" class="form-control     validator " group-validator="grupo1" requiredFm="true" validatorMssg="Por favor digitar Password" type="password"  />
                            </div>
                        </div>
                        
                        <div class="form-group">
                            <label class="col-md-2">
                               Confirmar Password</label>
                            <div class="col-md-8">
                                <input  id="txtConfirm" class="form-control     validator " group-validator="grupo1" requiredFm="true" validatorMssg="Por favor confirmar el password" type="password" />
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-2">
                               Perfil</label>
                            <div class="col-md-8">
                                <select id="slProfiles"  class="form-control validator" group-validator="grupo1" requiredFm="true" validatorMssg="Por favor seleccionar el perfil" >
                                <option value="">Seleccionar</option>
                                </select >                                
                            </div>
                        </div>
                </div>

        
          </div>
      <div class="modal-footer">
        <button type class="btn btn-success" id="btnRegister">Guardar</button>
        <button type class="btn btn-default" id="btnClean">Limpiar</button>
        <button type="button" class="btn btn-danger" data-dismiss="modal">Salir</button>        
      </div>
    </div><!-- /.modal-content -->
  </div><!-- /.modal-dialog -->
</div><!-- /.modal -->