<p id="notice"><%= notice %></p>

<h1>Pais</h1>
   <div class="box">
            <!-- /.box-header -->
            <div class="box-body">
<table class="table table-bordered table-striped filterTable" >
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Id</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pais.each do |pai| %>
      <tr>
        <td><%= pai.id_pais %></td>
        <td><%= pai.nombre_pais %></td>
        <td><%= link_to 'Show', pai %></td>
        <td><%= link_to 'Edit', edit_pai_path(pai) %></td>
        <td><%= link_to 'Destroy', pai, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
</div>
<br>

<%= link_to 'New Pai', new_pai_path, { class: "btn btn-success btn-sm" }%>
