<script src="/JsViews/visitaAuditor.js" type="text/javascript"></script>
<div class='row'>
<div class='col-md-3'>
<div class="box box-solid">
            <div class="box-header with-border">
              <h4 class="box-title">Busqueda</h4>
            </div>
            <div class="box-body">
              <!-- the events -->
              <div id="external-events">
                <div class="external-event  ui-draggable ui-draggable-handle" style="position: relative;">
				 <select id="year"  class="form-control validator" group-validator="grupo1" requiredFm="true" validatorMssg="Por favor seleccionar el año" >
                    <option value="">Seleccionar Año</option>
                </select > 
				</div>
				
              <div class="external-event  ui-draggable ui-draggable-handle" style="position: relative;">
				 <select id="month"  class="form-control validator" group-validator="grupo1" requiredFm="true" validatorMssg="Por favor seleccionar el mes" >
                    <option value="">Seleccionar Mes</option>
					<option value="01">Enero</option>
					<option value="02">Febrero</option>
					<option value="03">Marzo</option>
					<option value="04">Abril</option>
					<option value="05">Mayo</option>
					<option value="06">Junio</option>
					<option value="07">Julio</option>
					<option value="08">Agosto</option>
					<option value="09">Septiembre</option>
					<option value="10">Octubre</option>
					<option value="11">Noviembre</option>
					<option value="12">Diciembre</option>
                </select > 
				</div>
                <div class="external-event  ui-draggable ui-draggable-handle" style="position: relative;">
				<button id="btnBuscar" class='btn btn-primary'>Buscar</button>
              </div>
            </div>
            <!-- /.box-body -->
          </div>
</div>
</div>
<div class='col-md-9'>
<div class="box box-primary">
<div class="box-body no-padding">
<div id="calendar" class="fc fc-ltr fc-unthemed" >
<div class="fc-view-container" style="">
	<div class="fc-view fc-month-view fc-basic-view" style="">

         <table>
			<thead>
				<tr>
					<td class="fc-widget-header">
						<div class="fc-row fc-widget-header">
						
						</div>
					</td>
				</tr>
			</thead>
			<tbody>
			   <tr>
                <td class="fc-widget-content">
                <div class="fc-day-grid-container" style="">
				<div class="fc-day-grid" id="containerCalendar">

				<div class="fc-toolbar">
				<div class="fc-center"><h2><label id="date"></h2></div>
				</div>



				</div>
				</div>
				</td>
			   </tr>
			</table>

	</div>
</div>

</div>
</div>
</div>
</div>
</div>

<div id="createVisita" class="modal fade" role="dialog" data-backdrop="static"> 
    <div class="modal-dialog">
        <div class="modal-content">	
            <div class="modal-header"> 
                <h4 class="modal-tittle">CREAR VISITA</h4>                
            </div> 
                <div class="modal-body"> 
				<input type="text" id="hiddenDay"/>
				<input type="text" id="hiddenMes"/>
				<input type="text" id="hiddenYear"/>
				<div class="form-horizontal">  
                   <div class="form-group"> 
				   <div class="col-md-4">
                        <i class="fa fa-users"></i><label> Empresa: </label>
                    </div>
                    <div class="col-md-8">
                        <select id="selectEmpresa" class="form-control  validator" group-validator="grupo2" requiredFm="true"  validatorMssg="Por favor digitar la sucursal">
                            <option value="">Seleccione..</option>
							<% @Empresas.each do |empresa| %>
                                <option  value="<%= empresa.id_empresa%>"><%= empresa.nombre_empresa%> </option>
                            <% end %>                            
                        </select>
                    </div>
                  </div>

				  <div class="form-horizontal">  
                   <div class="form-group"> 
				   <div class="col-md-4">
                        <i class="fa fa-users"></i><label> Sucursal: </label>
                    </div>
                    <div class="col-md-8">
                        <select id="selectSucursal" class="form-control  validator" group-validator="grupo2" requiredFm="true"  validatorMssg="Por favor digitar la sucursal">
                            <option value="">Seleccione..</option>                            
                        </select>
                    </div>
                  </div>
				
				</div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">
                        <span class="glyphicon glyphicon-remove"></span><span class="hidden-xs"> Cerrar</span>
                    </button>
                    <button type="submit" id="btnRegister"  class="btn btn-primary">
                        <span class="fa fa-save"></span><span class="hidden-xs"> Guardar</span>
                              
                    </button>
                </div>
                
        </div>
    </div>
</div>
