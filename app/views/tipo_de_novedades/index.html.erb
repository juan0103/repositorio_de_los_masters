<p id="notice"><%= notice %></p>
<section class="content-header">
  <h1>Tipo De Novedades</h1>    
</section>   
<div class="box">
  <div class="box-body">
    <table class="table table-bordered table-striped filterTable">
      <thead>
        <tr>
          <th>Id tnovedad</th>
          <th>Desc tnovedad</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @tipo_de_novedades.each do |tipo_de_novedade| %>
          <tr>
            <td><%= tipo_de_novedade.id_tnovedad %></td>
            <td><%= tipo_de_novedade.desc_tnovedad %></td>
            <td><%= link_to 'Show', tipo_de_novedade %></td>
            <td><%= link_to 'Edit', edit_tipo_de_novedade_path(tipo_de_novedade) %></td>
            <td><%= link_to 'Destroy', tipo_de_novedade, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
    </div>
  </div>
<br>

